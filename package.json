{
  "name": "worldmap_3d",
  "version": "6.0.2",
  "description": "3D Cesium and babylonjs based client",
  "homepage": "https://mholthausen.github.io/worldmap_3d/",
  "license": "Apache-2.0",
  "author": {
    "name": "terrestris GmbH & Co. KG",
    "url": "https://www.terrestris.de/en"
  },
  "contributors": [
    {
      "name": "Michael Holthausen",
      "email": "holthausen@terrestris.de",
      "url": "https://www.github.com/mholthausen"
    }
  ],
  "engines": {
    "npm": ">=10",
    "node": ">=22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mholthausen/worldmap_3d.git"
  },
  "type": "module",
  "scripts": {
    "test": "npm run check",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext js,jsx,ts,tsx src/ --cache --quiet",
